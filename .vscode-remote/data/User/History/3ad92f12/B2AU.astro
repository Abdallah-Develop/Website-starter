---
const {
      title,
        url,
          noArrow = false,
            class: additionalClass = "",
            } = Astro.props

---


<div class="lines absolute inset-0"></div>

<style>
  .lines {
    background: repeating-linear-gradient(
      to right,
      transparent,
      transparent 6px,
      var(--color-primary) 8px,
      var(--color-primary) 6px
    );
    opacity: 1;
    pointer-events: none;
    z-index: 1;
  }
</style>
